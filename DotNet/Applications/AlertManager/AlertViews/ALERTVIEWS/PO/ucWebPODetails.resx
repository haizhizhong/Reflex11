  <data name="sqlSelectCommand_daWS_PCPO_Hdr.CommandText" xml:space="preserve">
    <value>select w.ws_pcpo_id, w.pri_id, w.field_po, w.po_no, w.po_date, w.required_date, w.supplier, t.DESCRIPTION AS POType, st.Description AS POSupplyType, 
	l1.lv1_code + ' - ' + l1.lv1_desc AS Level1, l2.lv2_code + ' - ' + l2.lv2_desc AS Level2, l3.lv3_code + ' - ' + l3.lv3_desc AS Level3, 
	l4.lv4_code + ' - ' + l4.lv4_desc AS Level4, w.supplier_name AS POName, w.po_addr1, w.po_addr2, w.po_city, w.po_state, w.po_zip, 
	c.DESCRIPTION AS Country, w.submission_comments, cast(p.pri_code as varchar(15)) + ' - '+p.pri_name [Project], s.NAME [SupplierName],
	isnull((select SUM(ISNULL(wd.extension,0)) from WS_PCPO_Det wd where wd.ws_pcpo_id=w.ws_pcpo_id),0) [PurchaseAmount]
from WS_PCPO_Hdr AS w 
left outer join SUPPLIER_MASTER s on s.SUPPLIER_ID=w.supplier_id
left outer join COUNTRIES AS c ON c.COUNTRY_ID = w.po_country_id 
left outer join PO_TYPE AS t ON t.TYPE = w.po_type 
left outer join PO_SupplyType AS st ON st.PO_SupplyType_ID = w.po_supply_type_id 
left outer join PROJ_HEADER p on p.pri_id=w.pri_id
left outer join Level1_Codes AS l1 ON l1.lv1ID = w.lv1id 
left outer join Level2_Codes AS l2 ON l2.lv2ID = w.lv2id 
left outer join Level3_Codes AS l3 ON l3.lv3ID = w.lv3id 
left outer join Level4_Codes AS l4 ON l4.lv4ID = w.lv4id
where (w.ws_pcpo_id = @ws_pcpo_id)</value>
  </data>
