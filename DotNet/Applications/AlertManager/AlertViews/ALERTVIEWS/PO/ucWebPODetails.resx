  <data name="sqlSelectCommand_daAP_NonPORoute.CommandText" xml:space="preserve">
    <value>select c.KnownAs, l.Level_ID, l.Level_Description, a.response, a.response_time, a.response_notes
from WF_Route AS h 
JOIN WF_RouteDet AS a ON a.WF_Route_ID = h.WF_Route_ID 
JOIN web_strike_test.dbo.Contact AS c ON c.ID = a.contact_id 
JOIN web_strike_test.dbo.WS_Approval_Levels AS l ON l.Approval_Level_ID = a.approval_level_id
where (h.Link_ID = @ws_pcpo_id) and h.WF_ApprovalPoint_ID=1
order by l.Level_ID, c.KnownAs</value>
  </data>
