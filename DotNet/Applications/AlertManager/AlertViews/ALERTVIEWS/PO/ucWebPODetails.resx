  <data name="sqlSelectCommand_daWS_PCPO_Det.CommandText" xml:space="preserve">
    <value>SELECT        w.description, 
                         CASE WHEN w.lem_comp = 'L' THEN 'Labor' WHEN w.lem_comp = 'E' THEN 'Equipment' WHEN w.lem_comp = 'M' THEN 'Material' WHEN w.lem_comp = 'O' THEN 'Other' WHEN w.lem_comp = 'S' THEN 'Sub-Contract'
                          ELSE '' END AS LEMComponent, w.qty_ordered, u.DESCRIPTION AS UnitOfMeasure, w.unit_price AS Amount, w.extension AS Total, w.reference
FROM            WS_PCPO_Det AS w LEFT OUTER JOIN
                         UNITS_OF_MEASURE AS u ON u.UNIT_OF_MEASURE_CODE = w.uom
WHERE        (w.ws_pcpo_id = @ws_pcpo_id)</value>
  </data>
